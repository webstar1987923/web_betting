{
  "name": "betsys",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "tsc & concurrently \"tsc -w\" \"lite-server\" ",
    "serve": "http-server . -p 5556",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "clean_appjs": "find app -name '*.js' -o -name '*.js.map' | grep -v '\\.shim\\.js$' | xargs rm -f",
    "clean": "npm run clean-compile && rm -rf ./dist/",
    "clean-compile": "find ./app \\( -name '*.ngfactory.ts' -o -name '*.ngfactory.js' -o -name '*.ngfactory.js.map' -o -name '*.metadata.json' \\) -exec rm -f {} + && find ./dist -name 'bundle.es2015.js' -exec rm -f {} \\;",
    "ngc": "ngc -p tsconfigc-ngc-es2015.json",
    "rollup": "rollup -f iife -c rollup.config.js -o dist/bundle.es2015.js 2> /dev/null",
    "es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js 2> /dev/null",
    "build-prodapp": "npm run clean && mkdir -p ./dist/ && mkdir -p ./dist/css/ && npm run ngc && tsc -p tsconfigc-es2015.json && npm run rollup && npm run es5 && npm run minify && npm run clean-compile",
    "build-prod-staticfiles": "cat prod-jslibs | xargs awk 'FNR==1{print \"\"}1' > dist/libs.js && cat prod-cssfiles | xargs awk 'FNR==1{print \"\"}1' > dist/css/styles.css && cp index-dist.html dist/index.html",
    "build-prod-copyassets": "cp -r ./static dist/",
    "build-prod": "npm run build-prodapp && npm run build-prod-staticfiles && npm run build-prod-staticminify && npm run build-prod-copyassets && npm run build-prod-clean",
    "build-prod-staticminify": "uglifyjs dist/libs.js --screw-ie8 --compress -mangle --output dist/libs.min.js 2> /dev/null && uglifycss dist/css/styles.css > dist/css/styles.min.css",
    "build-prod-clean": "find dist/ \\( -name '*.css' -o -name '*.js' \\) | grep -v '\\.min\\.\\(css\\|js\\)$' | xargs rm -f"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "^2.4.4",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "@angular/upgrade": "~2.2.0",
    "@types/core-js": "~0.9.34",
    "@types/jasmine": "~2.5.35",
    "@types/node": "~6.0.45",
    "@types/protractor": "~1.5.20",
    "@types/selenium-webdriver": "~2.44.0",
    "angular-in-memory-web-api": "~0.1.15",
    "angular2-busy": "1.0.1",
    "angular2-dynamic-component": "0.0.50",
    "angularfire2": "^2.0.0-beta.6",
    "babel-core": "^6.22.1",
    "bootstrap": "^3.3.7",
    "bootstrap.native": "^1.1.0",
    "core-js": "^2.4.1",
    "del": "2.2.0",
    "font-awesome": "^4.7.0",
    "gulp": "3.9.1",
    "gulp-clean-css": "2.0.7",
    "gulp-concat": "2.6.0",
    "gulp-live-server": "0.0.29",
    "gulp-plumber": "1.1.0",
    "gulp-sass": "2.3.1",
    "gulp-sass-lint": "1.1.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "5.0.0",
    "gulp-typescript": "2.13.4",
    "gulp-uglify": "1.5.3",
    "jquery": "^3.1.1",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "ng2-bootstrap": "^1.1.16",
    "ng2-bs3-modal": "^0.10.4",
    "ng2-dnd": "^2.2.2",
    "ng2-translate": "^4.2.0",
    "reflect-metadata": "^0.1.8",
    "run-sequence": "1.2.0",
    "rxjs": "5.0.0-beta.12",
    "rxjs-es": "^5.0.0-beta.12",
    "systemjs": "0.19.39",
    "systemjs-builder": "0.15.33",
    "ts-metadata-helper": "0.0.3",
    "tsconfig-glob": "0.4.3",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "@angular/compiler-cli": "~2.2.0",
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "browserify": "^13.0.1",
    "canonical-path": "0.0.2",
    "concurrently": "^3.0.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.2",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "tslint": "^3.15.1",
    "typescript": "~2.0.3",
    "uglifyjs": "^2.4.10"
  }
}
